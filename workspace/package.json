{
    "name": "smile-cycle",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "serve:browse": "DEBUG=-* vue-cli-service serve",
        "serve:api": "DEBUG=$DEBUG,-engine*,-express* nodemon -L ./srv/server.js",
        "serve:ws": "DEBUG=$DEBUG,-engine* nodemon -L ./srv/ws.js",
        "serve": "DEBUG=-* node ./srv/server.js",
        "serve:debug": "DEBUG=*,-engine* nodemon -L ./srv/server.js",
        "build": "DEBUG=-* vue-cli-service build",
        "lint": "vue-cli-service lint"
    },
    "dependencies": {
        "@mdi/font": "^3.6.95",
        "@vue/cli": "^4.1.2",
        "axios": "^0.19.2",
        "bcrypt": "^3.0.7",
        "connect-history-api-fallback": "^1.6.0",
        "connect-mongo": "^3.2.0",
        "connect-redis": "^4.0.4",
        "core-js": "^3.6.4",
        "debug": "^4.1.1",
        "express": "^4.17.1",
        "express-session": "^1.17.0",
        "file-stream-rotator": "^0.5.7",
        "moment": "^2.24.0",
        "moment-timezone": "^0.5.27",
        "mongoose": "^5.8.11",
        "morgan": "^1.9.1",
        "multer": "^1.4.2",
        "nodemailer": "^6.4.2",
        "passport": "^0.4.1",
        "passport-local": "^1.0.0",
        "redis": "^2.8.0",
        "roboto-fontface": "*",
        "socket.io": "^2.3.0",
        "socket.io-client": "^2.3.0",
        "vue": "^2.6.11",
        "vue-router": "^3.1.5",
        "vuetify": "^2.2.9"
    },
    "devDependencies": {
        "@vue/cli-plugin-babel": "^4.1.2",
        "@vue/cli-plugin-eslint": "^4.1.2",
        "@vue/cli-plugin-router": "^4.1.2",
        "@vue/cli-service": "^4.1.2",
        "babel-eslint": "^10.0.3",
        "eslint": "^5.16.0",
        "eslint-plugin-vue": "^5.0.0",
        "nodemon": "^2.0.2",
        "vue-cli-plugin-vuetify": "^2.0.4",
        "vue-template-compiler": "^2.6.11"
    },
    "eslintConfig": {
        "root": true,
        "env": {
            "node": true
        },
        "extends": [
            "plugin:vue/essential",
            "eslint:recommended"
        ],
        "rules": {
            "no-console": 0
        },
        "parserOptions": {
            "parser": "babel-eslint"
        }
    },
    "postcss": {
        "plugins": {
            "autoprefixer": {}
        }
    },
    "browserslist": [
        "> 1%",
        "last 2 versions"
    ],
    "nodemonConfig": {
        "restartable": "rs",
        "watch": [
            "srv/"
        ]
    }
}
